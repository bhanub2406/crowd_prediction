        pd.DataFrame(FeatureHasher(n_features=650,input_type=\"string\").transform(df['train_name']).toarray()),\n","        pd.DataFrame(FeatureHasher(n_features=10,input_type=\"string\").transform(df['country_code_source']).toarray()),\n","        pd.DataFrame(FeatureHasher(n_features=10,input_type=\"string\").transform(df['country_code_destination']).toarray()),\n","        pd.DataFrame(FeatureHasher(n_features=7,input_type=\"string\").transform(df['current_day']).toarray())]\n","    ,axis = 1)\n","    df = df.drop(['id_code', 'current_date', 'current_time','source_name', 'destination_name', 'train_name',\n","        'country_code_source', 'country_code_destination', 'current_day', \n","        'is_weekend', 'target']\n","        , axis = 1)\n","    return df\n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"OoFkBNeFFSvT","colab_type":"code","outputId":"2ea74624-2de8-4450-ec10-b74251f1cdb0","executionInfo":{"status":"ok","timestamp":1573235272138,"user_tz":-330,"elapsed":1047,"user":{"displayName":"Bhanu sree Balisetty","photoUrl":"https://lh3.googleusercontent.com/a-/AAuE7mDP21Fovgv-54GdUi8eB52GjzNw-SFegRrRjvMChg=s64","userId":"06056630683889706511"}},"colab":{"base_uri":"https://localhost:8080/","height":85}},"source":["from sklearn.model_selection import train_test_split\n","X = data\n","y = data.target\n","X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.20, stratify=y)\n","\n","print(X_train.shape)\n","print(y_train.shape)\n","print(X_test.shape)\n","print(y_test.shape)"],"execution_count":0,"outputs":[{"output_type":"stream","text":["(1027, 19)\n","(1027,)\n","(257, 19)\n","(257,)\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"ScT_q8zvZBHK","colab_type":"code","colab":{}},"source":["# Additional features\n","X_train = extract_features(X_train, 'train')\n","X_test = extract_features(X_test)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"X8EJgDku8QGi","colab_type":"code","